{
  "name": "mckenziearts/laravel-oauth",
  "version": "1.0.4",
  "description": "Social OAuth authentication for Laravel 5. Drivers: Facebook, Twitter, Google, LinkedIn, Github",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Monney",
      "email": "monneylobe@gmail.com",
      "homepage": "https://www.twitter.com/monneyarthur",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mckenziearts/laravel-oauth",
  "keywords": ["Laravel", "Laravel-socialite", "oauth"],
  "require": {
    "illuminate/support": "~5",
    "laravel/socialite": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Mckenziearts\\LaravelOAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mckenziearts\\LaravelOAuth\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mckenziearts\\LaravelOAuth\\LaravelOAuthServiceProvider"
      ],
      "aliases": {
        "LaravelSocialite": "Mckenziearts\\LaravelOAuth\\Facades\\LaravelSocialite"
      }
    }
  }
}